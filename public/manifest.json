{
  "manifest_version": 2,
  "version": "1.0",
  "name": "CRA test",
  "description": "CRAで作ってみた",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}